<%- include('partials/header') %>

<div class="container-fluid main">
    <div style="height: 700px; text-align: left; width: 1600px; overflow-y:scroll; border: 1px solid rgb(187, 187, 187);">
        <ul class="list-group">
            
        </ul>
        <form action="/features/verrifiedpass" method="post">
          <table class="table">
            <thead>
              <tr>
                <th scope="col">UID</th>
                <th scope="col">Name</th>
                <th scope="col">Room No</th>
                <th scope="col">Place</th>
                <th scope="col">Out Details</th>
                <th scope="col">In Details</th>
                <th scope="col"></th>
              </tr>
            </thead>
            <tbody>
                <% list.forEach((detail)=>{ %>
                    <tr>
                      <td><strong><%= detail.uidData %></strong></td>
                      <td><%= detail.name %></td>
                      <td><%= detail.hostelDetails.roomNo %></td>
                      <td><%= detail.place %></td>
                      <td><%= detail.outTime %></td>
                      <td><%= detail.inTime %></td>
                      <td><button class="btn btn-primary" style="font-size: 0.7rem; margin: 0; background-color: rgb(0, 115, 255);" type="submit" name="uidByButton" value=<%= detail.uidData %>>View</button></td>
                    </tr>
                <% }) %>
            </tbody>
          </table>
        </form>
    </div>
</div>