<%- include('partials/header') %>

    <div class="container-fluid main">
        <div class="container-fluid contents">
            <h1 class="titleText">All Generated Passes</h1>
            <form class="form-2" method="post" action="/guardResult">
                <table class="table">
                    <tbody>
                        <tr>
                            <td>
                                <div class="mb-3">
                                    <label class="form-label">Name</label>
                                    <h2>
                                        <%= studentName %>
                                    </h2>
                                </div>
                            </td>
                            <td style="width: 400px;">
                                <div class="mb-3">
                                    <label class="form-label">UID</label>
                                    <h2>
                                        <%= uidData %>
                                    </h2>
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <div class="mb-3">
                                    <label class="form-label">Mobile No</label>
                                    <h2>
                                        <%= mobileNo %>
                                    </h2>
                                </div>
                            </td>
                            <td style="width: 400px;">
                                <div class="mb-3">
                                    <label class="form-label">Guiardian's No</label>
                                    <h2>
                                        <%= gurdianNo %>
                                    </h2>
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <div class="mb-3">
                                    <label class="form-label">Purpose</label>
                                    <h2>
                                        <%= purposeData %>
                                    </h2>
                                </div>
                            </td>
                            <td style="width: 400px;">
                                <div class="mb-3">
                                    <label class="form-label">Place</label>
                                    <h2>
                                        <%= placeData %>
                                    </h2>
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <div class="mb-3">
                                    <label class="form-label">Out Time</label>
                                    <h2>
                                        <%= outTimeData %>
                                    </h2>
                                </div>
                            </td>
                            <td style="width: 400px;">
                                <div class="mb-3">
                                    <label class="form-label">Out Date</label>
                                    <h2>
                                        <%= outDateData %>
                                    </h2>
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <div class="mb-3">
                                    <label class="form-label">Hostel Name</label>
                                    <h2>
                                        <%= hostelName %>
                                    </h2>
                                </div>
                            </td>
                            <td style="width: 400px;">
                                <div class="mb-3">
                                    <label class="form-label">Room No</label>
                                    <h2>
                                        <%= roomNo %>
                                    </h2>
                                </div>
                            </td>
                        </tr>

                    </tbody>
                </table>
            </form>

        </div>
    </div>

</body>

</html>