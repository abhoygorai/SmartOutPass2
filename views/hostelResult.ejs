<%- include('partials/header') %>

    <div class="container-fluid main">
        <div class="container-fluid contents">

            <h1 class="titleText">Student Details</h1>
            <form class="form-2" method="post" action="/hostelresult">
                <table class="table table-borderless table-1">
                    <tr>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">Name</label>
                                <input required type="text" class="form-control" name="nameData"
                                    value="<%= studentName %>">
                            </div>
                        </td>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">UID</label>
                                <input required type="text" class="form-control" name="uidData" value=<%=uidData %>>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">Mobile No</label>
                                <input required type="text" class="form-control" name="mobileNo" value=<%=mobileNo %>>
                            </div>
                        </td>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">Guiardian's No</label>
                                <input required type="text" class="form-control" name="gurdianNo" value=<%=gurdianNo %>>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">Hostel Name</label>
                                <input required type="text" class="form-control" name="hostelName" value=<%=hostelName
                                    %>>
                            </div>
                        </td>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">Hostel Name</label>
                                <input required type="text" class="form-control" name="roomNo" value=<%=roomNo %>>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">Place</label>
                                <input required type="text" class="form-control" name="placeData">
                            </div>
                        </td>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">Purpose</label>
                                <input required type="text" class="form-control" name="purposeData">
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">Out Time</label>
                                <input required type="time" class="form-control" name="outTimeData">
                            </div>
                        </td>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">Out Date</label>
                                <input required type="date" class="form-control" name="outDateData">
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="mb-3">
                                <label class="form-label">In Time</label>
                                <input required type="time" class="form-control" name="inTimeData">
                            </div>
                        </td>

                        <td>
                            <div class="mb-3">
                                <label class="form-label">In Date</label>
                                <input required type="date" class="form-control" name="inDateData">
                                <input hidden type="text" class="form-control" name="wardenData" value="wardenData">
                            </div>
                        </td>
                    </tr>
                </table>
                <button class="btn btn-danger" id="auth-submit-button" type="submit">Submit</button>
            </form>

        </div>
    </div>

    </body>

    </html>